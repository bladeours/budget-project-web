type TransactionsPage {
  content: [Transaction]
  number: Int
  size: Int
  totalElements: Int
  totalPages: Int
}

input TransactionInput {
  name: String!
  amount: Float!
  date: String!
  need: Boolean!
  accountToHash: String
  accountFromHash: String
  transactionType: TransactionType!
  categoryHash: String
  subCategoryHash: String
  currency: Currency!
  note: String
}


type Transaction {
  name: String
  note: String
  amount: Float
  date: String
  need: Boolean
  hash: String
  accountTo: Account
  accountFrom: Account
  transactionType: TransactionType
  category: Category
  subCategory: Category
}

enum TransactionType {
  INCOME
  EXPENSE
  TRANSFER
}
